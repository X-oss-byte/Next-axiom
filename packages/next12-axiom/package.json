{
  "name": "next12-axiom",
  "description": "Send WebVitals from your Next.js project to Axiom.",
  "version": "0.18.0",
  "author": "Axiom, Inc.",
  "license": "MIT",
  "contributors": [
    "Arne Bahlo <arne@axiom.co>",
    "Islam Shehata <islam@axiom.co>"
  ],
  "engines": {
    "node": ">=16"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "exportTypes": "tsc -b -v",
    "build": "tsup",
    "format": "prettier --write src/*.ts test/*.ts",
    "lint": "prettier -c src/*.ts test/*.ts",
    "test": "jest",
    "clean": "rm -rf dist tsconfig.tsbuildinfo"
  },
  "tsup": {
    "entry": ["src/**/*.ts"],
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "dts": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axiomhq/next-axiom.git"
  },
  "keywords": [
    "web-vitals",
    "axiom",
    "observability",
    "performance",
    "nextjs"
  ],
  "bugs": {
    "url": "https://github.com/axiomhq/next-axiom/issues"
  },
  "homepage": "https://github.com/axiomhq/next-axiom#readme",
  "peerDependencies": {
    "next": ">=12.2.0 <13.0.0"
  },
  "devDependencies": {
    "next-axiom-core": "workspace:*"
  }
}